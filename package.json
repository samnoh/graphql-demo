{
    "name": "graphql-demo",
    "version": "1.0.0",
    "description": "GraphQL Demo",
    "private": true,
    "main": "src/app.js",
    "scripts": {
        "start": "NODE_ENV=production PORT=80 pm2 start build/app.js -i 0",
        "dev": "nodemon --exec babel-node src/app.js ",
        "build": "npm run clean && babel src --out-dir build --copy-files",
        "clean": "rm -rf build/",
        "kill": "pm2 kill"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samnoh/graphql-demo.git"
    },
    "keywords": [
        "GraphQL",
        "graphql-yoga"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/samnoh/graphql-demo/issues"
    },
    "homepage": "https://github.com/samnoh/graphql-demo#readme",
    "devDependencies": {
        "@babel/cli": "^7.5.0",
        "@babel/core": "^7.5.4",
        "@babel/node": "^7.5.0",
        "@babel/preset-env": "^7.5.4",
        "nodemon": "^1.19.1"
    },
    "dependencies": {
        "dotenv": "^8.0.0",
        "graphql-import": "^0.7.1",
        "graphql-yoga": "^1.18.1",
        "mongoose": "^5.6.4",
        "pm2": "^3.5.1"
    }
}
